<?xml version="1.0" encoding="UTF-8" standalone="no"?>
  <ExtensionManifest ExtensionBundleId="mure" ExtensionBundleName="mure" ExtensionBundleVersion="0.1.9" Version="5.0">
    <Author>Alex Bigelow</Author>
    <Contact mailto="alex.r.bigelow@gmail.com" />
    <Abstract href="http://mure-apps.github.io">
      <![CDATA[
A widget that syncs Illustrator selections and changes with the rest of the Mure ecosystem.
    ]]>
    </Abstract>
    <ExtensionList>
      <Extension Id="mure.extension" Version="1.0" />
    </ExtensionList>
    <ExecutionEnvironment>
      <HostList>
        <Host Name="ILST" Version="[20.0,30.9]" />
      </HostList>
      <LocaleList>
        <Locale Code="All" />
      </LocaleList>
      <RequiredRuntimeList>
        <RequiredRuntime Name="CSXS" Version="7.0" />
      </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
      <Extension Id="mure.extension">
        <DispatchInfo>
          <Resources>
            <MainPath>./index.html</MainPath>
          </Resources>
          <UI>
            <Type>Panel</Type>
            <Menu>Mure</Menu>
            <Geometry>
              <Size>
                <Height>500</Height>
                <Width>775</Width>
              </Size>
              <MinSize>
                <Height>500</Height>
                <Width>535</Width>
              </MinSize>
            </Geometry>
            <Icons>
              <Icon Type="Normal">./img/icon2.png</Icon>
              <Icon Type="RollOver">./img/icon2.png</Icon>
              <Icon Type="Disabled">./img/icon.png</Icon>
              <Icon Type="DarkNormal">./img/icon.png</Icon>
              <Icon Type="DarkRollOver">./img/icon.png</Icon>
            </Icons>
          </UI>
        </DispatchInfo>
      </Extension>
    </DispatchInfoList>
  </ExtensionManifest>
